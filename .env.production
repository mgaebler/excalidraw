# not needed for self hosted
VITE_APP_PORTAL_URL=
VITE_APP_AI_BACKEND=
VITE_APP_PLUS_LP=

# production-only vars
VITE_APP_GOOGLE_ANALYTICS_ID=

VITE_APP_PLUS_APP=

VITE_APP_ENABLE_TRACKING=false
