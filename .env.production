VITE_APP_HTTP_STORAGE_WS_URL_PART_NAME=draw-room

# use fixed service url
# VITE_APP_BACKEND_V2_GET_URL=http://localhost:5011
# VITE_APP_BACKEND_V2_POST_URL=http://localhost:5011
# VITE_APP_HTTP_STORAGE_BACKEND_URL=http://localhost:5011
VITE_APP_PLUS_LP=https://plus.excalidraw.com
VITE_APP_PLUS_APP=https://app.excalidraw.com

VITE_APP_AI_BACKEND=

# socket server URL used for collaboration
VITE_APP_WS_SERVER_URL=http://localhost:5012

VITE_APP_LIBRARY_URL=https://libraries.excalidraw.com
